# Uncomment the next line to define a global platform for your project

source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'

target 'SimpsonsCharacterViewer' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SimpsonsCharacterViewer
   pod 'Alamofire', '~> 4.7'
   pod 'Alamofire-SwiftyJSON'
   pod 'SwiftyJSON', '~> 4.0'
   pod 'Kingfisher', '~> 4.0'


end

target 'TheWireCharacterViewer' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TheWireCharacterViewer
   pod 'Alamofire', '~> 4.7'
   pod 'Alamofire-SwiftyJSON'
   pod 'SwiftyJSON', '~> 4.0'
   pod 'Kingfisher', '~> 4.0'

end
